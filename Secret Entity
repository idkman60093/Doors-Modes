require(game.Players.LocalPlayer.PlayerGui.MainUI.Initiator.Main_Game).caption("You just added an extra entity called Him", true)
wait(2)
require(game.Players.LocalPlayer.PlayerGui.MainUI.Initiator.Main_Game).caption("Original Credits to Chrono for Him Entity", true)
wait(2)
require(game.Players.LocalPlayer.PlayerGui.MainUI.Initiator.Main_Game).caption("This is just Recreated Version of Him Entity so it's not the same as the original one's", true)
wait(2)
require(game.Players.LocalPlayer.PlayerGui.MainUI.Initiator.Main_Game).caption("Good Luck!", true)

local entityList = {
{url =
"https://raw.githubusercontent.com/idkman60093/Doors-Modes/refs/heads/main/Him%20Incoming%20Sound", delay = 40},
{url =
"https://raw.githubusercontent.com/idkman60093/Doors-Modes/refs/heads/main/Him%20Entity%20(Recreated)", delay = 115},
}

for _, entity in ipairs(entityList) do
coroutine.wrap(function()
while true do
wait(entity.delay)
pcall(function()
loadstring(game:HttpGet(entity.url))()
end)
end
end)()
end
