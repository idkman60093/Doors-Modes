wait(math.random(150,250))
local spawner=loadstring(game:HttpGet("https://raw.githubusercontent.com/RegularVynixu/Utilities/main/Doors/Entity%20Spawner/V2/Source.lua"))()
local e=spawner.Create({
Entity={Name="Silence",Asset="https://github.com/sofianmohamedovich-cmyk/tes/blob/main/silence.rbxm?raw=true",HeightOffset=0},
Lights={Flicker={Enabled=true,Duration=10},Shatter=true,Repair=false},
Earthquake={Enabled=false},
